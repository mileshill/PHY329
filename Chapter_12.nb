     RowBox[{
      RowBox[{"Reap", "[", 
       RowBox[{"Do", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"x", "[", "i", "]"}], "=", 
           RowBox[{
            RowBox[{"Inverse", "[", "d", "]"}], ".", 
            RowBox[{"(", 
