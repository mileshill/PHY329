Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"$Line", "=", "0"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"Traj", "[", 
     RowBox[{"\[Theta]0_", ",", 
      RowBox[{"\[CapitalDelta]x_:", "90."}], ",", 
      RowBox[{"v0_:", "30."}], ",", 
      RowBox[{"\[CapitalDelta]y_:", "-", "0.8"}], ",", 
      RowBox[{"g_:", "9.8"}]}], "]"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{"Tan", "[", "\[Theta]0", "]"}], "\[CapitalDelta]x"}], "-", 
     RowBox[{
      FractionBox["g", 
       RowBox[{"2", 
